<logging>
  <filter enabled="true">
    <tag>stdout</tag>
    <type>console</type>
    <!-- level is (:?FINEST|FINE|DEBUG|TRACE|INFO|WARNING|ERROR) -->
    <level>DEBUG</level>
  </filter>
  <filter enabled="false">
    <tag>file</tag>
    <type>file</type>
    <level>DEBUG</level>
    <property name="filename">test.log</property>
  </filter>
</logging>
